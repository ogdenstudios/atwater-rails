<h1><%= @filter %></h1>
<div class="book__container">
  <% @books.each do |book| %>
    <div class="book" data-service="<%= book.service %>">
      <li><%=book.title%></li>
      <div class="author__image-wrapper">
        <%= image_tag book.picture.url, class: "author__image" %>
      </div>
    </div>
  <% end %>
</div>
